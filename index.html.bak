  <body>
    <div class="navbar navbar-fixed-top">
      <div class="navbar-inner">
        <div class="container">
          <a class="brand" href="#">AppConfig Spec Creator</a>
        </div>
      </div>
    </div>
    <div class="container" id="formContainer">
      <div class="row clearfix">
        <!-- Building Form. -->
        <div class="span7">
          <div class="clearfix topPadded">
            <h2 class="inline">Your Form</h2>
            <div id="downloadSpec" class="inline topSpecButton"></div>
            <div id="importSpec" class="inline topSpecButton"></div>
            <div id="addGroup" class="rightPadded topSpecButton"></div>
            <hr>
            <div id="build" class="formScrollDiv scrollable">
              <form id="target" class="form-horizontal">
              </form>
            </div>
          </div>
        </div>
        <!-- / Building Form. -->

        <!-- Components -->
        <div class="inflow">
        <div class="positioner">
        <div class="fixed span6" id="componentSection">
          <h2>Drag & Drop components</h2>
          <hr>
          <div class="tabbable">
            <ul class="nav nav-tabs" id="formtabs">
              <!-- Tab nav -->
            </ul>
            <form class="form-horizontal" id="components">
              <fieldset>
                <div class="tab-content">
                  <!-- Tabs of snippets go here -->
                </div>
              </fieldset>
            </form>
          </div>
        </div>
        </div>
        </div>
        <!-- / Components -->

      </div>

    </div> <!-- /container -->

